		<view hlookat="-93.804" vlookat="-6.653" fovtype="MFOV" fov="99.978" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="panos/DSC_0754_Panorama.tiles/preview.jpg" />

		<image type="CUBE" multires="true" tilesize="512" if="!webvr.isenabled">
			<level tiledimagewidth="1024" tiledimageheight="1024">
				<cube url="panos/DSC_0754_Panorama.tiles/%s/l1/%v/l1_%s_%v_%h.jpg" />
			</level>
		</image>

		<image if="webvr.isenabled">
			<cube url="panos/DSC_0754_Panorama.tiles/vr/pano_%s.jpg" />
		</image>

		<!-- place your scene hotspots here -->

	</scene>


</krpano>
